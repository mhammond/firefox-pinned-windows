// Most if this is probably unused while this is an embedded extension.
{
  "manifest_version": 2,
  "name": "Pinned Windows",
  "version": "1.1",

  "description": "Allows a window to be 'pinned' so new links will not be opened in that window.",
  "homepage_url": "https://github.com/mhammond/firefox-pinned-windows",

  "background": {
    "scripts": ["pinned-window.js"]
  },

  "permissions": [
    "tabs",
    "webNavigation",
    "contextMenus"
  ]
}
