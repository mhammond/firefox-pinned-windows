<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>
    <em:id>pinned-window@mhammond.github.com</em:id>
    <em:name>Pinned Windows</em:name>
    <em:version>1.11</em:version>
    <em:description>Allows a window to be 'pinned' so new links will not be opened in that window.</em:description>
    <em:creator>Mark Hammond</em:creator>
    <!-- optional items -->
    <em:homepageURL>https://github.com/mhammond/firefox-pinned-windows</em:homepageURL>
    <em:hasEmbeddedWebExtension>true</em:hasEmbeddedWebExtension>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <!-- Firefox 54 is the first with onCreatedNavigationTarget -->
        <em:minVersion>54.0</em:minVersion>
        <em:maxVersion>*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:multiprocessCompatible>true</em:multiprocessCompatible>
  </Description>

</RDF>
